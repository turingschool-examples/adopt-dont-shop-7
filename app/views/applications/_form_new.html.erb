<%= form_with url: path, method: method, data: {turbo: false} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :street_address, "Street Address" %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip_code, "Zip Code" %>
  <%= f.text_field :zip_code %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit button_text %>
<% end %>