<h1>New Application</h1>
<%= form_with url: "/applications", local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state%>
  
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code%>

  <%= f.label :description, "Description of why I would make a good home" %>
  <%= f.text_field :description%>

  <%= f.submit :submit %>
<% end %>
